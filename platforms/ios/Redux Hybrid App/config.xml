<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.hellocordova" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="AllowInlineMediaPlayback" value="false" />
    <preference name="BackupWebStorage" value="cloud" />
    <preference name="DisallowOverscroll" value="false" />
    <preference name="EnableViewportScale" value="false" />
    <preference name="KeyboardDisplayRequiresUserAction" value="true" />
    <preference name="MediaPlaybackRequiresUserAction" value="false" />
    <preference name="SuppressesIncrementalRendering" value="false" />
    <preference name="SuppressesLongPressGesture" value="false" />
    <preference name="Suppresses3DTouchGesture" value="false" />
    <preference name="GapBetweenPages" value="0" />
    <preference name="PageLength" value="0" />
    <preference name="PaginationBreakingMode" value="page" />
    <preference name="PaginationMode" value="unpaginated" />
    <feature name="LocalStorage">
        <param name="ios-package" value="CDVLocalStorage" />
    </feature>
    <feature name="HandleOpenUrl">
        <param name="ios-package" value="CDVHandleOpenURL" />
        <param name="onload" value="true" />
    </feature>
    <feature name="IntentAndNavigationFilter">
        <param name="ios-package" value="CDVIntentAndNavigationFilter" />
        <param name="onload" value="true" />
    </feature>
    <feature name="GestureHandler">
        <param name="ios-package" value="CDVGestureHandler" />
        <param name="onload" value="true" />
    </feature>
    <feature name="SplashScreen">
        <param name="ios-package" value="CDVSplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <feature name="InAppBrowser">
        <param name="ios-package" value="CDVInAppBrowser" />
    </feature>
    <allow-intent href="itms:*" />
    <allow-intent href="itms-apps:*" />
    <icon height="57" src="icons/ios/icon-57.png" width="57" />
    <icon height="72" src="icons/ios/icon-72.png" width="72" />
    <icon height="76" src="icons/ios/icon-76.png" width="76" />
    <icon height="114" src="icons/ios/icon-57-2x.png" width="114" />
    <icon height="120" src="icons/ios/icon-120.png" width="120" />
    <icon height="144" src="icons/ios/icon-72-2x.png" width="144" />
    <icon height="152" src="icons/ios/icon-76-2x.png" width="152" />
    <splash height="480" src="splashes/ios/Default~iphone.png" width="320" />
    <splash height="960" src="splashes/ios/Default@2x~iphone.png" width="640" />
    <splash height="1136" src="splashes/ios/Default-568h@2x~iphone.png" width="640" />
    <splash height="1334" src="splashes/ios/Default-667h.png" width="750" />
    <splash height="2208" src="splashes/ios/Default-736h.png" width="1242" />
    <hook src="hooks/build.sh" type="before_prepare" />
    <name>Redux Hybrid App</name>
    <description>
        A demo for Hybrid App starter for using Redux+React on Cordova platform.
    </description>
    <author email="zhangsichu@gmail.com" href="https://gmail.com">
        ZhangSichu
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="ShowSplashScreen" value="false" />
    <preference name="SplashScreenDelay" value="1000" />
    <preference name="prerendered-icon" value="true" />
    <icon src="icon.png" />
</widget>
